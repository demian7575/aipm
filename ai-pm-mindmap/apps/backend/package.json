{
  "name": "@ai-pm-mindmap/backend",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsup src/server.ts --format esm,cjs --dts --out-dir dist",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "lint": "eslint src --ext .ts",
    "generate:openapi": "ts-node src/openapi.ts",
    "seed": "ts-node src/seed/reset.ts"
  },
  "dependencies": {
    "@ai-pm-mindmap/shared": "workspace:^1.0.0",
    "cors": "2.8.5",
    "express": "4.18.2",
    "pino": "8.17.0",
    "pino-http": "8.5.0",
    "swagger-ui-express": "5.0.0",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/node": "20.11.30",
    "@types/supertest": "2.0.16",
    "@types/uuid": "9.0.7",
    "supertest": "6.3.4",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "tsx": "4.7.1",
    "tsup": "7.2.0",
    "typescript": "5.4.2",
    "vitest": "1.4.0"
  }
}
