{
  "name": "@ai-pm-mindmap/backend",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsup src/server.ts --dts",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:unit": "vitest run --dir tests/unit",
    "test:integration": "vitest run --dir tests/integration",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "generate:openapi": "ts-node src/scripts/generate-openapi.ts",
    "seed": "ts-node src/scripts/reset-seed.ts"
  },
  "dependencies": {
    "@ai-pm-mindmap/shared": "workspace:*",
    "cors": "2.8.5",
    "express": "4.19.2",
    "http-errors": "2.0.0",
    "morgan": "1.10.0",
    "nanoid": "5.0.7",
    "swagger-ui-express": "5.0.1"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/http-errors": "2.0.4",
    "@types/morgan": "1.9.9",
    "@types/node": "20.12.7",
    "@vitest/coverage-v8": "1.5.0",
    "supertest": "6.4.2",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "tsup": "8.0.2",
    "typescript": "5.4.5",
    "vitest": "1.5.0"
  }
}
