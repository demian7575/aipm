{
  "version": 1,
  "framework": { "name": "custom", "version": "1.0.0" },
  "routes": [
    { "path": "/api/*", "target": { "kind": "Compute", "src": "default" } },
    { "path": "/*.*",   "target": { "kind": "Static" } },
    { "path": "/*",     "target": { "kind": "Static" } }
  ],
  "computeResources": [
    { "name": "default", "runtime": "nodejs22.x", "entrypoint": "server-backend.js" }
  ]
}

