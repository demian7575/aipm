{
  "name": "@ai-pm/backend",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsup src/server.ts --dts --format esm --out-dir dist",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "test": "vitest run",
    "test:watch": "vitest",
    "generate:openapi": "tsx src/openapi-script.ts",
    "seed": "tsx src/seed.ts"
  },
  "dependencies": {
    "@ai-pm/shared": "workspace:*",
    "@fastify/cors": "9.0.1",
    "@fastify/sensible": "5.5.0",
    "@fastify/swagger": "8.12.1",
    "@fastify/swagger-ui": "1.10.2",
    "fastify": "4.28.1",
    "pino": "9.1.0",
    "pino-pretty": "11.2.1",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@types/node": "20.12.7",
    "@types/pino": "8.6.1",
    "@typescript-eslint/eslint-plugin": "7.10.0",
    "@typescript-eslint/parser": "7.10.0",
    "ts-node": "10.9.2",
    "tsup": "7.2.0",
    "tsx": "4.7.1",
    "typescript": "5.4.5",
    "vitest": "1.5.0"
  }
}
