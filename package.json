{
  "name": "ai-pm-mindmap",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.15.4",
  "description": "AI Project Manager Mindmap monorepo",
  "workspaces": ["apps/*", "packages/*"],
  "scripts": {
    "dev": "pnpm --recursive --parallel dev",
    "build": "pnpm --recursive build",
    "lint": "pnpm --recursive lint",
    "test": "pnpm --recursive test",
    "e2e": "pnpm --filter @ai-pm/frontend e2e",
    "preview": "pnpm --filter @ai-pm/frontend preview",
    "generate:openapi": "pnpm --filter @ai-pm/backend generate:openapi",
    "seed": "pnpm --filter @ai-pm/backend seed"
  }
}
